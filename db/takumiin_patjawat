;</div>\r\n<div>const app = electron.app;</div>\r\n<div>const BrowserWindow = electron.BrowserWindow;</div>\r\n<div>const path = require(\'path\');</div>\r\n<div>const url = require(\'url\');</div>\r\n<div>const isDev = require(\'electron-is-dev\');</div>\r\n<div>let mainWindow;</div>\r\n<div>function createWindow() {</div>\r\n<div>mainWindow = new BrowserWindow({</div>\r\n<div>width: 900,</div>\r\n<div>height: 680</div>\r\n<div>});</div>\r\n<div>mainWindow.loadURL(isDev ? \'http://localhost:3000\' : `file://${path.join(__dirname, \'../build/index.html\')}`);</div>\r\n<div>mainWindow.on(\'closed\', () =&gt; mainWindow = null);</div>\r\n<div>}</div>\r\n<div>app.on(\'ready\', createWindow);</div>\r\n<div>app.on(\'window-all-closed\', () =&gt; {</div>\r\n<div>if (process.platform !== \'darwin\') {</div>\r\n<div>app.quit();</div>\r\n<div>}</div>\r\n<div>});</div>\r\n<div>app.on(\'activate\', () =&gt; {</div>\r\n<div>if (mainWindow === null) {</div>\r\n<div>createWindow();</div>\r\n<div>}</div>\r\n<div>});</div>\r\n</div>\r\n<div>แก้ไข package.json</div>\r\n<div>\r\n<div>\r\n<div>\"main\": \"public/electron.js\",</div>\r\n</div>\r\n</div>\r\n<div>\r\n<div>\r\n<div>\"scripts\": {</div>\r\n<div>\"start\": \"react-scripts start\",</div>\r\n<div>\"build\": \"react-scripts build &amp;&amp; yarn electron-build\",</div>\r\n<div>\"electron-build\": \"electron-builder\",</div>\r\n<div>\"test\": \"react-scripts test --env=jsdom\",</div>\r\n<div>\"eject\": \"react-scripts eject\",</div>\r\n<div>\"electron-dev\": \"concurrently \\\"BROWSER=none yarn start\\\"\\\"wait-on http://localhost:3000 &amp;&amp; electron .\\\"\"</div>\r\n<div>},</div>\r\n</div>\r\n</div>','รวมร่าง ElectronJS + ReactJS  สร้าง Desktop App','','pending','open','open','','%e0%b8%a3%e0%b8%a7%e0%b8%a1%e0%b8%a3%e0%b9%88%e0%b8%b2%e0%b8%87-electronjs-reactjs-%e0%b8%aa%e0%b8%a3%e0%b9%89%e0%b8%b2%e0%b8%87-desktop-app','','','2018-08-10 12:10:56','2018-08-10 12:10:56','',0,'http://www.patjawat.com/?p=167',0,'post','',0),