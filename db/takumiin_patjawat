  `download_status` varchar(200) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `download_status_message` varchar(200) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `meta_data` longtext COLLATE utf8mb4_unicode_ci,
